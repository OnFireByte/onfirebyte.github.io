        "astro": "^2.10.14",
        "playwright": "^1.37.1",
        "start-server-and-test": "^2.0.0",
        "svelte": "^4.0.0",
        "tailwindcss": "^3.0.24",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.9",
        "@tsconfig/node16": "^16.1.1"
